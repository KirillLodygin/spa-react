{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": [
    "react-app",
    "prettier"
  ],
  "rules": {
    "jsx-quotes": [
      1,
      "prefer-double"
    ],
    "no-var": "error",
    "semi": "error",
    "no-multi-spaces": "error",
    "space-in-parens": "error",
    "no-multiple-empty-lines": "error",
    "prefer-const": "error"
  },
  "plugins": [
    "@typescript-eslint",
    "eslint-plugin-prettier",
    "eslint-plugin-unicorn" ,
    "eslint-plugin-import"
  ]
}
